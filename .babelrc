{
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "regenerator": true
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
   "production": {
     "presets": [
       [
         "@babel/preset-env", {
           "targets": "last 2 versions"
         }
       ],
       "@babel/preset-react"
     ]
   },
   "development": {
     "presets": [
       "@babel/preset-env",
       "@babel/preset-react"
     ],
     "plugins": [
       "@babel/plugin-syntax-dynamic-import"
     ]
   }
  }
}
