{
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "regenerator": true
      }
    ]
  ],
  "env": {
   "production": {
     "presets": [
       [
         "@babel/preset-env", {
           "targets": "last 2 versions"
         }
       ],
       "@babel/preset-react"
     ]
   },
   "development": {
     "presets": [
       "@babel/preset-env",
       "@babel/preset-react"
     ]
   }
  }
}
